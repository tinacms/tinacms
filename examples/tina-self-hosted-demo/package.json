    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "tinacms": "workspace:*",
    "tinacms-gitprovider-github": "workspace:*",
    "tinacms-authjs": "workspace:*"
