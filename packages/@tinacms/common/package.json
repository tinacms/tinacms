{
  "name": "@tinacms/common",
  "version": "1.0.0",
  "description": "Common types needed for tinacms packages.",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "./dist/index.mjs",
  "files": [
  	"package.json",
  	"dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "buildConfig": {
    "entryPoints": [
      "src/index.ts"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "url": "https://github.com/tinacms/tinacms.git",
    "directory": "packages/@tinacms/common"
  },
  "scripts": {
    "build": "tsc"
  },
  "devDependencies": {
    "typescript": "^5.7.3"
  }
}
